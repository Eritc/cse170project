<!doctype html>

<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href = "css/bootstrap.css" rel="stylesheet">
	<link href = "css/bootstrap-theme.css" rel = "stylesheet">
	<link href = "css/hci-friends.css" rel = "stylesheet">
	<link href = "css/bootstrap-twitter.css" rel = "stylesheet">
	<script>
	function myFunction(id)
	{
		if(id == "nav-home")
		{
			document.getElementById("home").innerHTML = "<img src = 'http://cse.ucsd.edu/sites/cse/files/cse/assets/images/EBU3BB.jpg' style = 'display:block; margin-left: auto; margin-right: auto;'> </img>"

		}
		if(id == "nav-labs")
		{
			document.getElementById("home").innerHTML="<title> Labs </title> <h1> Coming Soon!! </h1>";
		}
		if(id == "nav-contact-us")
		{
			document.getElementById("home").innerHTML = "<title> Contact Us </title> <h1> UCSD CSE 170 class </h1>";
		}
	}
</script>

